{
  "name": "@wikipathways/kaavio",
  "version": "3.7.5-beta",
  "description": "Diagram viewer and editor for biological pathways.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.",
  "typings": "lib/index",
  "files": [
    "lib",
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "npm-run-all build:*",
    "build:dist": "rimraf ./dist && webpack --config webpack.prod.config.js",
    "build:lib": "npm run compile",
    "compile": "rimraf ./lib && tsc --project tsconfig.json && npm run assets",
    "compile-broken:xsd1": "rm -rf cache/* xmlns/* && cxsd https://cdn.rawgit.com/ariutta/GPML/9a8cb502c8745058c0611106f33a0f383819b3f0/GPML2013a.xsd",
    "compile-broken:xsd": "rm -rf cache/* xmlns/* && cxsd http://www.w3.org/TR/2002/WD-SVG11-20020108/SVG.xsd",
    "assets": "npm run gulp copy:assets",
    "test:e2e": "npm run serve && npm run serve:bundle",
    "serve": "webpack-dev-server --config webpack.dev.config.js --open --progress --inline",
    "start": "npm run serve",
    "test": "echo 'get some tests working'",
    "gulp": "gulp",
    "watch": "rimraf ./lib && npm run assets && npm-run-all watch:*",
    "watch:compile": "tsc --project tsconfig.json --watch"
  },
  "browser": {
    "commander": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikipathways/kaavio.git"
  },
  "keywords": [
    "kaavio",
    "network",
    "diagram"
  ],
  "author": "WikiPathways team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wikipathways/kaavio/issues"
  },
  "homepage": "https://wikipathways.github.io/kaavio/docs/",
  "dependencies": {
    "JSONStream": "^1.3.1",
    "commander": "^2.11.0",
    "csstips": "^0.2.0",
    "csx": "^8.2.1",
    "fast-levenshtein": "^2.0.6",
    "getit": "^1.1.0",
    "highland": "^2.11.1",
    "js-base64": "^2.1.9",
    "json-stringify-safe": "^5.0.1",
    "leven": "^2.1.0",
    "lodash": "^4.14.36",
    "ndjson": "^1.5.0",
    "opentype-layout": "^1.1.0",
    "opentype.js": "^0.7.3",
    "os-fonts": "^0.4.1",
    "point-at-length": "^1.0.2",
    "react": "^15.6.0",
    "react-addons-css-transition-group": "^15.6.0",
    "react-addons-shallow-compare": "^15.4.1",
    "react-dom": "^15.5.4",
    "react-selectize": "^2.1.0",
    "react-spinkit": "^3.0.0",
    "roboto-fontface": "^0.7.0",
    "rx-extra": "^2.0.3",
    "rxjs": "^5.2.0",
    "source-map-support": "^0.4.17",
    "svg-pan-zoom": "^3.5.2",
    "svg-points": "^6.0.0",
    "typestyle": "^0.22.3",
    "url-loader": "^0.5.8",
    "url-regex": "^4.1.1",
    "valid-data-url": "^0.1.4",
    "verror": "^1.10.0",
    "web-animations-js": "^2.2.2",
    "word-wrappr": "^1.0.2",
    "xhr2": "^0.1.4",
    "xmldom": "^0.1.27",
    "xpath": "0.0.24"
  },
  "devDependencies": {
    "@types/js-base64": "^2.1.5",
    "@types/node": "^6.0.73",
    "@types/react": "^15.0.28",
    "@types/react-dom": "^0.14.23",
    "copyfiles": "^1.2.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^3.4.0",
    "gulp-concat-css": "^2.3.0",
    "gulp-copy": "^1.0.0",
    "npm-run-all": "^2.1.1",
    "rimraf": "^2.6.1",
    "style-loader": "^0.16.1",
    "svg-url-loader": "^2.0.2",
    "ts-loader": "^2.3.6",
    "typescript": "2.2.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^1.16.5",
    "webpack-hot-middleware": "^2.18.0"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [
        ".git",
        "jsdoc-conf.json",
        "npm-debug.log"
      ],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./lib",
        "./demo",
        "./gulp",
        "./dist"
      ]
    }
  }
}
