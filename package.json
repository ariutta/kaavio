{
  "name": "kaavio",
  "version": "4.0.0-10",
  "description": "Diagram viewer and editor for biological pathways.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "jsnext:main": "lib/index.js",
  "typings": "lib/index",
  "engines": {
    "node": ">=6.11.4"
  },
  "files": [
    "bin",
    "lib",
    "src",
    "dist",
    "tsconfig.json",
    "tsconfig.*.json",
    "yarn.lock",
    "webpack.*.config.js"
  ],
  "bin": "./bin/kaavio",
  "scripts": {
    "build": "npm-run-all -p build:*",
    "build:dist": "rm -rf ./dist && npm-run-all -p build:dist:*",
    "build:dist:browser": "webpack --config webpack.browser.dist.config.js",
    "build:dist:node": "webpack --config webpack.node.dist.config.js",
    "build:lib": "rm -rf ./lib && npm-run-all -p build:lib:*",
    "build:lib:browser": "webpack --config webpack.browser.lib.config.js",
    "build:lib:node": "webpack --config webpack.node.lib.config.js",
    "placeholders": "npm-run-all placeholders:*",
    "placeholders:dirs": "mkdir -p src/drawers/edges src/drawers/filters src/drawers/icons src/drawers/markers src/drawers/styles",
    "placeholders:edges": "echo \"export const a = 1;\" > src/drawers/edges/__bundled_dont_edit__.tsx",
    "placeholders:filters": "echo \"export const Highlight = function(color) {return {filterProperties: {id: 'abc'}, filterPrimitives: []}};\" > src/drawers/filters/__bundled_dont_edit__.tsx",
    "placeholders:icons": "echo \"import * as React from 'React'; export class Icons extends React.Component<any, any> {render(){return <div />}};\" > src/drawers/icons/__bundled_dont_edit__.tsx ",
    "placeholders:markers": "echo \"export const a = 1;\" > src/drawers/markers/__bundled_dont_edit__.tsx",
    "placeholders:styles": "echo \"export const a = 1;\" > src/drawers/styles/__bundled_dont_edit__.tsx",
    "postinstall-TODO-should-we-keep-this": "npm run placeholders && npm dedupe",
    "prepare": "npm run placeholders && npm run build:dist:node && ./bin/kaavio bundle --build false edges && ./bin/kaavio bundle --build false filters && ./bin/kaavio bundle icons --build false && ./bin/kaavio bundle markers --build false && npm run build && npm run test",
    "serve": "webpack-dev-server --config webpack.lib.dev.config.js --open --progress --inline",
    "start": "npm run serve",
    "setExpected": "echo 'Setting expected values for tests' && npm-run-all setExpected:* && echo ok || echo not ok",
    "setExpected:checksums": "for f in ./test/inputs/*.json; do base_path=`echo \"$f\" | sed s/.json//`; echo \"Converting $f to SVG and generating checksum...\"; cat \"$f\" | bin/kaavio json2svg -s false | shasum -p -a 1 > \"$base_path.svg.sha1sum\"; done && echo ok || echo not ok",
    "delete-me:convert-gpml": "for f in ./test/inputs/*.gpml.xml; do base_path=`echo \"$f\" | sed s/.xml// | sed s/.gpml//`; cat \"$f\" | ../gpml2pvjson-js/bin/gpml2pvjson > \"$base_path.json\"; done && echo ok || echo not ok",
    "test": "echo 'get in-browser tests working' && npm run test:checksums",
    "test:e2e": "npm run serve && npm run serve:bundle",
    "test:checksums": "for f in ./test/inputs/*.json; do base_path=`echo \"$f\" | sed s/.json//`; echo \"Verifying checksum for $base_path.svg...\"; cat \"$f\" | bin/kaavio json2svg -s false | shasum -p -a 1 -c \"$base_path.svg.sha1sum\"; done && echo ok || echo not ok",
    "watch": "rm -rf ./dist && npm-run-all -p watch:*",
    "watch:browser": "webpack --config webpack.browser.dev.config.js --watch",
    "watch:node": "webpack --config webpack.node.dev.config.js --watch"
  },
  "browser": {
    "commander": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikipathways/kaavio.git"
  },
  "keywords": [
    "kaavio",
    "network",
    "diagram"
  ],
  "author": "WikiPathways team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wikipathways/kaavio/issues"
  },
  "homepage": "https://wikipathways.github.io/kaavio/docs/",
  "dependencies": {
    "JSONStream": "^1.3.1",
    "collit": "^1.0.1",
    "color-interpolate": "^1.0.2",
    "commander": "^2.11.0",
    "csstips": "^0.2.0",
    "csx": "^8.2.1",
    "direction": "^1.0.1",
    "fast-levenshtein": "^2.0.6",
    "fs-extra": "^4.0.2",
    "getit": "^1.1.0",
    "highland": "^2.11.1",
    "is-valid-var-name": "^1.0.2",
    "js-base64": "^2.3.2",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.14.36",
    "ndjson": "^1.5.0",
    "opentype-layout": "^1.1.0",
    "opentype.js": "^0.7.3",
    "os-fonts": "^0.4.1",
    "parent-package-json": "^2.0.1",
    "point-at-length": "^1.1.0",
    "react": "^15.6.2",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dom": "^15.6.2",
    "react-selectize": "^2.1.0",
    "react-spinkit": "^3.0.0",
    "rgb2hex": "^0.1.0",
    "roboto-fontface": "^0.7.0",
    "rx-extra": "^2.0.3",
    "rxjs": "^5.2.0",
    "source-map-support": "^0.4.18",
    "style-loader": "^0.16.1",
    "svg-pan-zoom": "^3.5.3",
    "svg-points": "^6.0.0",
    "typestyle": "^0.22.3",
    "url-regex": "^4.1.1",
    "valid-data-url": "^0.1.4",
    "verror": "^1.10.0",
    "web-animations-js": "^2.2.2",
    "word-wrappr": "^1.0.2",
    "xhr2": "^0.1.4",
    "xmldom": "^0.1.27",
    "xpath": "0.0.24"
  },
  "devDependencies": {
    "@types/js-base64": "^2.1.5",
    "@types/node": "^8.0.41",
    "@types/react": "github:ariutta/types-react-v15#50edb5d",
    "@types/react-dom": "^15.5.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "npm-run-all": "^4.1.1",
    "string-replace-webpack-plugin": "^0.1.3",
    "ts-loader": "^2.3.7",
    "typescript": "^2.5.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-hot-middleware": "^2.20.0"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [
        ".git",
        "jsdoc-conf.json",
        "npm-debug.log"
      ],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./lib",
        "./demo",
        "./dist"
      ]
    }
  }
}
