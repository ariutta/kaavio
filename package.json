{
  "name": "kaavio",
  "version": "4.0.0",
  "description": "Diagram viewer and editor for biological pathways.",
  "main": "es5/index.js",
  "module": "esnext/index.js",
  "jsnext:main": "esnext/index.js",
  "typings": "esnext/index",
  "engines": {
    "node": ">=6.11.4"
  },
  "files": [
    "bin",
    "es5",
    "esnext",
    "src",
    "dist",
    "yarn.lock",
    "webpack.*.config.js"
  ],
  "bin": "./bin/kaavio",
  "scripts": {
    "build": "rm -rf dist/ es5/ esnext/; npm-run-all -r -s build:css -p build:assets -s build:ts",
    "build:assets": "npm-run-all -p build:assets:*",
    "build:assets:es5": "rsync -a --delete --include='*.json' --include='*.svg' --include='*.css' --include='*.css.d.ts' --include='*/' --exclude='*' src/ es5/",
    "build:assets:esnext": "rsync -a --delete --include='*.json' --include='*.svg' --include='*.css' --include='*.css.d.ts' --include='*/' --exclude='*' src/ esnext/",
    "build:css": "tcm src",
    "build:ts": "npm-run-all -p build:ts:*",
    "build:ts:dist": "webpack --config webpack.dist.config.js",
    "build:ts:es5": "tsc --project tsconfig.es5.json",
    "build:ts:esnext": "tsc --project tsconfig.esnext.json",
    "postinstall-TODO-should-we-keep-this": "npm run bundle && npm dedupe",
    "prepare-broken": "npm run bundle && npm run build:dist:node && ./bin/kaavio bundle --build false edges && ./bin/kaavio bundle --build false filters && ./bin/kaavio bundle defs --build false && ./bin/kaavio bundle markers --build false && npm run build && npm run test && npm run expected",
    "prepare-disabled": "npm run bundle && npm run build:dist:node && ./bin/kaavio bundle --build false edges && ./bin/kaavio bundle --build false filters && ./bin/kaavio bundle defs --build false && ./bin/kaavio bundle markers --build false && npm run build",
    "prepare": "npm run build",
    "serve": "webpack-dev-server --config webpack.dev.config.js --open --progress --inline",
    "start": "npm run serve",
    "expected": "for f in ./test/inputs/*.json; do base_path=`echo \"$f\" | sed s/.json//`; echo \"Converting $f to SVG and generating checksum...\"; cat \"$f\" | node test/dummy-cli.js | shasum -p -a 1 > \"$base_path.svg.sha1sum\" || break; done && echo ok || echo not ok",
    "testBuild": "echo 'testBuild requires rsync and the gpml2pvjson NPM package'; npm run build:ts:es5; ./bin/kaavio bundle ./test/dummy-themes/silly.json; tsc --jsx react ./test/dummy-themes/silly/theme.tsx; rsync -rIl --existing ../gpml2pvjson-js/test/expected/ test/inputs/",
    "test": "echo 'get in-browser tests working'; npm-run-all -p test:*",
    "test:checksums": "for f in ./test/inputs/*.json; do base_path=`echo \"$f\" | sed s/.json//`; echo \"Verifying checksum for $base_path.svg...\"; cat \"$f\" | node test/dummy-cli.js | shasum -p -a 1 -c \"$base_path.svg.sha1sum\" || break; done",
    "test:convert": "for f in ./test/inputs/*.json; do base_path=`echo \"$f\" | sed s/.json//`; echo \"Verifying that converting sample JSON files to SVG doesn't throw an error for $base_path.svg...\"; cat \"$f\" | node test/dummy-cli.js > /dev/null || break; done",
    "test-broken:jest": "echo 'Get the jest test(s) working (again-ish).' && jest",
    "test-broken:e2e": "npm run serve && npm run serve:bundle",
    "watch-old": "rm -rf ./es5 ./dist ./esnext && npm-run-all -s build:assets -p watch:*",
    "watch": "npm-run-all -r -s build -p watch:*",
    "watch:css": "tcm src --watch",
    "watch:assets": "onchange 'src/**' -- npm-run-all build:assets",
    "watch:es5": "tsc --project tsconfig.es5.json --watch",
    "watch:testBuild": "npm-run-all -p watch:testBuild:*",
    "watch:testBuild:bundle": "onchange './test/dummy-themes/**/*.json' './test/dummy-themes/**/*.svg' -- ./bin/kaavio bundle ./test/dummy-themes/silly.json",
    "watch:testBuild:ts": "tsc --jsx react ./test/dummy-themes/silly/theme.tsx --watch",
    "watch:testBuild:css": "tcm test/dummy-themes/ --watch",
    "watch:esnext": "tsc --project tsconfig.esnext.json --watch",
    "watch-disabled:browser": "echo 'Cannot view anything in browser for just kaavio'; webpack --config webpack.dev.config.js --watch",
    "watch-broken:test": "onchange 'es5/**' -- npm test"
  },
  "browser": {
    "commander": false
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wikipathways/kaavio.git"
  },
  "keywords": [
    "kaavio",
    "network",
    "diagram"
  ],
  "author": "WikiPathways team",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/wikipathways/kaavio/issues"
  },
  "homepage": "https://wikipathways.github.io/kaavio/docs/",
  "dependencies": {
    "collit": "^1.0.1",
    "color-interpolate": "^1.0.2",
    "commander": "^2.11.0",
    "csstips": "^0.2.0",
    "csx": "^8.2.1",
    "direction": "^1.0.1",
    "fast-levenshtein": "^2.0.6",
    "fs-extra": "^4.0.2",
    "getit": "^1.1.0",
    "highland": "^2.11.1",
    "is-valid-var-name": "^1.0.2",
    "js-base64": "^2.3.2",
    "json-stringify-safe": "^5.0.1",
    "lodash": "^4.14.36",
    "ndjson": "^1.5.0",
    "opentype-layout": "^1.1.0",
    "opentype.js": "^0.7.3",
    "os-fonts": "^0.4.1",
    "parent-package-json": "^2.0.1",
    "point-at-length": "^1.1.0",
    "prettier": "^1.9.0",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-addons-shallow-compare": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-selectize": "^3.0.1",
    "react-spinkit": "^3.0.0",
    "read-pkg-up": "^3.0.0",
    "relative-luminance": "0.0.2",
    "rgb2hex": "^0.1.0",
    "roboto-fontface": "^0.7.0",
    "rx-extra": "^2.0.3",
    "rxjs": "^5.2.0",
    "source-map-support": "^0.4.18",
    "style-loader": "^0.16.1",
    "svg-pan-zoom": "^3.5.3",
    "svg-points": "^6.0.0",
    "svgo": "^1.0.4",
    "typestyle": "^0.22.3",
    "url-regex": "^4.1.1",
    "valid-data-url": "^0.1.4",
    "verror": "^1.10.0",
    "web-animations-js": "^2.2.2",
    "word-wrappr": "^1.0.2",
    "xhr2": "^0.1.4",
    "xmldom": "^0.1.27",
    "xpath": "0.0.24",
    "xpath.js": "^1.1.0"
  },
  "devDependencies": {
    "@types/js-base64": "^2.1.5",
    "@types/node": "^8.0.41",
    "@types/react": "github:ariutta/types-react-v15#50edb5d",
    "@types/react-dom": "^15.5.7",
    "JSONStream": "^1.3.2",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-to-json": "^3.3.1",
    "expose-loader": "^0.7.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "jest": "^22.1.4",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.2.1",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "react-test-renderer": "^16.2.0",
    "string-replace-webpack-plugin": "^0.1.3",
    "to-string-loader": "^1.1.5",
    "ts-loader": "^2.3.7",
    "typed-css-modules": "^0.3.1",
    "typescript": "^2.5.3",
    "uglifyjs-webpack-plugin": "^0.4.6",
    "webpack": "^3.7.1",
    "webpack-dev-server": "^2.9.2",
    "webpack-hot-middleware": "^2.20.0"
  },
  "semverBumper": {
    "jsonFiles": [
      {
        "filepath": "package.json",
        "key": "version"
      }
    ],
    "grepOptions": {
      "include": [],
      "exclude": [
        ".git",
        "jsdoc-conf.json",
        "npm-debug.log"
      ],
      "excludeDir": [
        "./docs",
        "./test",
        "./bower_components",
        "./es5",
        "./esnext",
        "./demo",
        "./dist"
      ]
    }
  }
}
