{
  "compilerOptions": {
    "declaration": true,
    "experimentalDecorators": true,
    "inlineSourceMap": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noEmitOnError": true,
    "noImplicitAny": false,
    "pretty": true
  },
  "include": [
    "src/**/*.ts*"
  ],
  "exclude": [
    "dist/**/*",
    "tmp/**/*",
    "src/experimental",
    "src/components/Marker/MarkerDefs.tsx"
  ]
}
